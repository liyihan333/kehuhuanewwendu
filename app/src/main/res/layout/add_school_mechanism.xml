<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/multi_title"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="#393F4D">
        <ImageView
            android:id="@+id/IV_back_list_multi"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/back"
            android:layout_marginTop="20dp"
            android:layout_alignParentBottom="true"/>

        <ImageView
            android:id="@+id/multi_commit"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:padding="16dp"
            android:src="@drawable/edit_commit"
            android:layout_marginTop="20dp"
            android:layout_alignParentBottom="true"
            />
    </RelativeLayout>
    <include layout="@layout/activity_schinfo_title_two" />
<ListView
    android:id="@+id/lv_add"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"/>
</LinearLayout>